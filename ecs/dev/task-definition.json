{
  "family": "relaybox-session-dev",
  "taskRoleArn": "arn:aws:iam::767397814726:role/relaybox-dev-ecs-roles-ECSTaskRole-ODOaUvU1yjQJ",
  "containerDefinitions": [
    {
      "name": "relaybox-session-dev",
      "essential": true,
      "memoryReservation": "256",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/relaybox-session-dev",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "/aws/ecs"
        }
      }
    }
  ]
}
